    "lint:fix": "yarn eslint:fix && yarn fmt:fix",
    "typecheck": "tsc --noEmit",
    "ci": "yarn lint && yarn typecheck",
    "prepare": "husky install",
    "generate-preview": "node scripts/generate-preview.js && yarn lint:fix && yarn typecheck"
  },
  "dependencies": {
