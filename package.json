  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint . --ignore-path .gitignore",
    "eslint:fix": "yarn eslint --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
