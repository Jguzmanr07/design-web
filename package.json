    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "glob": "^9.3.4",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-css-order": "^1.3.0",
    "typescript": "*",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
